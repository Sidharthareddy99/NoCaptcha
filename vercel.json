{
    "version": 2,
    "builds": [
      {
        "src": "Backend/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "my-app/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Backend/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "/Frontend/build/$1"
      }
    ]
  }